{
  "name": "@xnestjs/ioredis",
  "version": "0.0.1",
  "description": "NestJS extension library for ioredis",
  "author": "Panates",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "prebuild": "npm run lint && npm run clean",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -b tsconfig-build-cjs.json",
    "build:esm": "tsc -b tsconfig-build-esm.json",
    "postbuild": "cp README.md package.json ../../LICENSE ../../build/ioredis && cp ../../package.cjs.json ../../build/ioredis/cjs/package.json",
    "lint": "eslint .",
    "test": "jest",
    "cover": "jest --collect-coverage",
    "clean": "npm run clean:src && npm run clean:dist && npm run clean:cover",
    "clean:src": "ts-cleanup -s src --all",
    "clean:dist": "rimraf ../../build/ioredis",
    "clean:cover": "rimraf ../../coverage/ioredis"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/testing": "^9.0.11",
    "@types/ioredis": "^4.28.10"
  },
  "peerDependencies": {
    "@nestjs/common": ">= 7.4.0",
    "ioredis": ">= 5.x.x"
  },
  "type": "module",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "default": "./esm/index.js"
    },
    "./cjs": "./cjs/index.js",
    "./esm": "./esm/index.js"
  },
  "engines": {
    "node": ">=16.0",
    "npm": ">=7.0.0"
  },
  "files": [
    "bin/",
    "cjs/",
    "esm/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "nestjs",
    "ioredis",
    "redis"
  ]
}
