{
  "name": "@xnestjs/storage",
  "version": "0.0.1",
  "description": "NestJS extension library for Storage solutions (S3,GS)",
  "author": "Panates",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "prebuild": "npm run lint && npm run clean",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -b tsconfig-build-cjs.json",
    "build:esm": "tsc -b tsconfig-build-esm.json",
    "postbuild": "cp README.md package.json ../../LICENSE ../../build/storage && cp ../../package.cjs.json ../../build/storage/cjs/package.json",
    "lint": "eslint . --max-warnings=0",
    "test": "jest",
    "cover": "jest --collect-coverage",
    "clean": "npm run clean:src && npm run clean:dist && npm run clean:cover",
    "clean:src": "ts-cleanup -s src --all",
    "clean:dist": "rimraf ../../build/storage",
    "clean:cover": "rimraf ../../coverage/storage"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.5",
    "@nestjs/core": "^10.0.5",
    "@nestjs/testing": "^10.0.5"
  },
  "peerDependencies": {
    "@nestjs/common": ">=8.0.0",
    "@nestjs/core": ">=8.0.0"
  },
  "optionalDependencies": {
    "minio": "^7.1.1"
  },
  "type": "module",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "default": "./esm/index.js"
    },
    "./*": {
      "require": "./cjs/*",
      "default": "./esm/*"
    }
  },
  "engines": {
    "node": ">=16.0",
    "npm": ">=7.0.0"
  },
  "files": [
    "bin/",
    "cjs/",
    "esm/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "nestjs",
    "storage",
    "s3"
  ]
}
