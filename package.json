{
  "name": "xnestjs",
  "description": "NestJS additional packages",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/xnestjs.git"
  },
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "preclean": "rimraf build coverage",
    "clean": "rman run clean",
    "build": "rman build",
    "test": "jest --passWithNoTests --detectOpenHandles",
    "precover": "rimraf coverage",
    "cover": "jest --passWithNoTests --detectOpenHandles --coverage",
    "precitest": "rimraf coverage",
    "citest": "jest --passWithNoTests --detectOpenHandles --coverage --coverageReporters=lcov"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-syntax-import-assertions": "^7.23.3",
    "@nestjs/platform-express": "^10.2.9",
    "@tsconfig/recommended": "^1.0.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.1",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "8.54.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "madge": "^6.1.0",
    "npm-run-path": "^5.1.0",
    "prettier": "^3.1.0",
    "putil-varhelpers": "^1.6.5",
    "rimraf": "^5.0.5",
    "rman": "^0.28.0",
    "ts-cleanup": "^0.2.6",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
