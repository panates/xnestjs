{
  "name": "xnestjs",
  "description": "NestJS additional packages",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/xnestjs.git"
  },
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "preclean": "rimraf build coverage",
    "clean": "rman run clean",
    "build": "rman build",
    "test": "jest --passWithNoTests --detectOpenHandles",
    "precover": "rimraf coverage",
    "cover": "jest --passWithNoTests --detectOpenHandles --coverage",
    "precitest": "rimraf coverage",
    "citest": "jest --passWithNoTests --detectOpenHandles --coverage --coverageReporters=lcov"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.7",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "antlr4ts": "^0.5.0-alpha.4",
    "eslint": "8.44.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.6.1",
    "npm-run-path": "^5.1.0",
    "prettier": "^3.0.0",
    "putil-varhelpers": "^1.6.5",
    "rimraf": "^5.0.1",
    "rman": "^0.28.0",
    "supertest": "^6.3.3",
    "ts-cleanup": "^0.2.6",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.4.0",
    "typescript": "^5.1.6"
  }
}
